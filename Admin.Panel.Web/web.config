<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <location path="." inheritInChildApplications="false">
        <system.webServer>
            <handlers>
                <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
            </handlers>
            <aspNetCore processPath="dotnet" arguments=".\Admin.Panel.Web.dll" stdoutLogEnabled="false"
                        stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
        </system.webServer>
        <system.webServer>
            <modules>
                <remove name="WebDAVModule" />
            </modules>
            <handlers>
                <remove name="WebDAV" />
            </handlers>
        </system.webServer>
    </location>
</configuration>
