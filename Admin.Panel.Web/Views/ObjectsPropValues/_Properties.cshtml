@using Admin.Panel.Core.Entities.Questionary
@using Microsoft.EntityFrameworkCore.Metadata.Internal
@model Admin.Panel.Core.Entities.Questionary.QuestionaryObject

@for (int i = 0; i < Model.SelectedObjectPropertyValues.Count; i++)
{
    <div class="form-group">
        <div class="col-md-10">

            <input type="hidden" asp-for="@Model.SelectedObjectPropertyValues[i].Id"/>
            <input type="hidden" asp-for="@Model.SelectedObjectPropertyValues[i].Name"/>
            <input type="hidden" asp-for="@Model.SelectedObjectPropertyValues[i].ObjectPropertyId"/>
            <label asp-for="@Model.SelectedObjectPropertyValues[i].Value">@Model.SelectedObjectPropertyValues[i].Name</label>
            <input asp-for="@Model.SelectedObjectPropertyValues[i].Value" class="form-control" required>
        </div>
    </div>
}